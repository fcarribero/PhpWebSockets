{
  "name": "webneex/php-websockets",
  "description": "Simple implementation class for Web Sockets in PHP",
  "license": "MIT",
  "type": "library",
  "require": {
    "php": ">=5.6.4",
    "ext-sockets": "*"
  },
  "autoload": {
